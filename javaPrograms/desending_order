var array= [50,20,40,30,10];
{

    for(var i=0;  i<array.length;  i++)
    {
        for(var j=i+1; j<array.length;  j++)
        {
            if(array[i]<array[j])
            {
                var temp=array[i];
                array[i]=array[j];
                array[j]=temp;		
            }
        }
    }
        console.log(array);



}
		
